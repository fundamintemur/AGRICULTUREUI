@model EntityLayer.Concrete.Adress
@{
    ViewData["Title"] = "EditTeam";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<div class="col-12 grid-margin stretch-card">
    <div class="card">
        <div class="card-body">
            <h4 class="card-title">Adres Güncelleme Sayfası</h4>

            <form class="forms-sample" method="post">
                <div class="form-group">
                    @Html.HiddenFor(x=>x.AdressId)
                    <label for="exampleInputName1">Açıklama 1</label>
                    <input type="text" class="form-control" id="exampleInputName1" placeholder="açıklama 1" asp-for="Description1">
                </div>
                <div class="form-group">
                    <label for="exampleInputEmail3">Açıklama 2</label>
                     <input type="text" class="form-control" id="exampleInputEmail3" placeholder="açıklama 2" asp-for="Description2">
                </div>

                <div class="form-group">
                    <label for="exampleInputEmail3">Açıklama 3</label>
                    <input type="text" class="form-control" id="exampleInputEmail3" placeholder="açıklama 3" asp-for="Description3">
                </div>
                <div class="form-group">
                    <label for="exampleInputEmail3">Açıklama 4</label>
                    <input type="text" class="form-control" id="exampleInputEmail3" placeholder="açıklama 4" asp-for="Description4">
                </div>
                
                <div class="form-group">
                    <label for="exampleInputEmail3">Harita Konumu</label>
                    <input type="text" class="form-control" id="exampleInputEmail3" placeholder="harita konumu" asp-for="MapInfo">
                </div>
                <button type="submit" class="btn btn-outline-primary me-2">Kaydet</button>
                @*  <a href="/Service/Index/" class="btn btn-outline-success">Vazgeç</a> *@
            </form>
        </div>
    </div>
</div>




